package kodlamaio;

public class Course {
	//attributes
	int courseID;
	String courseName;
	
	String teacherName;
	String courseInfo;
	
	//constructor
	public Course(int cid, String cname, String tname, String cinfo) {
		this.courseID 		= cid;
		this.courseName 	= cname;
		this.teacherName 	= tname;
		this.courseInfo		= cinfo;
		
		System.out.println("Course is created: "+this.courseName);
	}
}
