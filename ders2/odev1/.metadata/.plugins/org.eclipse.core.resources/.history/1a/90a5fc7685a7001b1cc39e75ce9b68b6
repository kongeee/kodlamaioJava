package kodlamaio;

public class MemberManager {
	public void editMember(Member member, String name, String e_mail) {
		System.out.println("Before edit : " + member.fullName+"/"+member.e_mail);
		
		member.fullName = name;
		member.e_mail = e_mail;
		
		System.out.println("After edit : " + member.fullName+"/"+member.e_mail);
	}
}
