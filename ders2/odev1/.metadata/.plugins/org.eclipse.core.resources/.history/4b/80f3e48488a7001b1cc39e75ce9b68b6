package kodlamaio;

public class Main {

	public static void main(String[] args) {
		Course course1 = new Course(1, "Java kursu", "Engin Demiroğ", "kurs detay");
		Member member1 = new Member(3, "Furkan Ekici", "furkanekici1997@gmail.com");
		CourseManager courseManager = new CourseManager();
		MemberManager memberManager = new MemberManager();
		
		courseManager.addToCourse(member1, course1);
		
		memberManager.editMember(member1, "Ekici", "furkanekici1997@gmail.com");
		

	}

}
